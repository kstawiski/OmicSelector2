[flake8]
max-line-length = 100
exclude =
    .git,
    __pycache__,
    venv,
    env,
    .venv,
    .eggs,
    *.egg,
    build,
    dist,
    .tox,
    .mypy_cache,
    .pytest_cache,
    docs,
    migrations
ignore =
    E203,
    E266,
    E501,
    W503,
    W504
max-complexity = 15
per-file-ignores =
    __init__.py:F401,F403
    tests/*:F401,F403,F811
